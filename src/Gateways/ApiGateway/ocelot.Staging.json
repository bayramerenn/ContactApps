{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.0",
      "DownstreamHostAndPorts": [
        {
          "Host": "contact-api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/c/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "UpstreamScheme": "http",
      "SwaggerKey": "contact",
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 30,
        "Limit": 50
      }
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.0",
      "DownstreamHostAndPorts": [
        {
          "Host": "report-api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/r/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "UpstreamScheme": "http",
      "SwaggerKey": "report",
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 30,
        "Limit": 50
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "contact",
      "Config": [
        {
          "Name": "Contact API",
          "Version": "v1.0",
          "Url": "http://contact-api:80/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "report",
      "Config": [
        {
          "Name": "Report API",
          "Version": "v1.0",
          "Url": "http://report-api:80/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigatway:80",
    "RateLimitOptions": {
      "DisableRateLimitHeaders": true,
      "QuotaExceededMessage": "You have made too many requests. Please try again later.",
      "HttpStatusCode": 429
    }
  }
}