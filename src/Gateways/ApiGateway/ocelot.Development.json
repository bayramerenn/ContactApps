{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.0",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7001"
        }
      ],
      "UpstreamPathTemplate": "/c/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "UpstreamScheme": "http",
      "SwaggerKey": "contact",
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 30,
        "Limit": 50
      }
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.0",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7002"
        }
      ],
      "UpstreamPathTemplate": "/r/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "UpstreamScheme": "http",
      "SwaggerKey": "report",
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 30,
        "Limit": 50
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "contact",
      "Config": [
        {
          "Name": "Contact API",
          "Version": "v1.0",
          "Url": "http://localhost:7001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "report",
      "Config": [
        {
          "Name": "Report API",
          "Version": "v1.0",
          "Url": "http://localhost:7002/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7000",
    "RateLimitOptions": {
      "DisableRateLimitHeaders": true,
      "QuotaExceededMessage": "You have made too many requests. Please try again later.",
      "HttpStatusCode": 429
    }
  }
}